{
  "name": "@semantic-terminal/core",
  "version": "0.1.0",
  "description": "The missing Semantic Layer for AI Agents to understand Terminal output",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./presets": {
      "types": "./dist/presets/index.d.ts",
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js"
    },
    "./parsers": {
      "types": "./dist/parsers/index.d.ts",
      "import": "./dist/parsers/index.mjs",
      "require": "./dist/parsers/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "terminal",
    "pty",
    "semantic",
    "ai",
    "mcp",
    "cli",
    "claude",
    "automation",
    "parser"
  ],
  "author": "Semantic Terminal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-terminal/semantic-terminal.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/semantic-terminal/semantic-terminal#readme",
  "bugs": {
    "url": "https://github.com/semantic-terminal/semantic-terminal/issues"
  },
  "dependencies": {
    "node-pty": "^1.0.0",
    "@xterm/headless": "^5.5.0",
    "@xterm/addon-serialize": "^0.13.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

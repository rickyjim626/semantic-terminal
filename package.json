{
  "name": "semantic-terminal",
  "version": "0.1.0",
  "private": true,
  "description": "The missing Semantic Layer for AI Agents to understand Terminal output",
  "repository": {
    "type": "git",
    "url": "https://github.com/semantic-terminal/semantic-terminal.git"
  },
  "homepage": "https://github.com/semantic-terminal/semantic-terminal#readme",
  "bugs": {
    "url": "https://github.com/semantic-terminal/semantic-terminal/issues"
  },
  "license": "MIT",
  "author": "Semantic Terminal",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:run": "turbo run test:run",
    "lint": "turbo run lint",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "turbo": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0"
  }
}
